# Doesn't work due to https://github.com/rvaiya/keyd/issues/619#issuecomment-3208123148
#minus = £

# Workaround:
minus = macro(compose L -)

# Worse workaround which relies on apps supporting C-S-u, and hence doesn't
# work in apps like emacs.
#minus = macro(C-S-u 0 0 a 3 enter)

[shift]
#minus = €
minus = macro(compose E =)

#minus = macro(C-S-u 2 0 a c enter)
