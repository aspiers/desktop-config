color-xterm*fg:			RGB:F/F/E
color-xterm*bg:			RGB:0/0/0
color-xterm*font:		10x20
color-xterm*geometry:		80x24+80+125
;color-xterm*visualBell:	true
color-xterm*scrollBar:		true
color-xterm*saveLines:		256
color-xterm.VT100.Translations: #override \
	<Key>Delete:    string(0x04)\n\
	<Key>Home:      string(0x1b) string("[1~")\n\
	<Key>End:       string(0x1b) string("[4~")
