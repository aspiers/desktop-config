! was keycode 0x25 =	Control_L
keycode 0x25 =	z		Z		guillemotleft	less		guillemotleft	less
! keycode 0x73 is Win key
keycode 0x73 = Alt_L		Meta_L
! keycode 0x40 is left Alt
keycode 0x40 =	Control_L
keycode 0x41 =	space
! keycode 0x71 is Alt Gr key, was ISO_Level3_Shift
keycode 0x71 =	Super_R
! was keycode 0x6D =	Control_R	Multi_key
keycode 0x6D =	ISO_Level3_Shift
keycode 0x62 =	Up
keycode 0x64 =	Left
keycode 0x68 =	Down
keycode 0x66 =	Right

clear Shift
clear Lock
clear Control
clear Mod1
clear Mod2
clear Mod3
clear Mod4
clear Mod5

add    Shift   = Shift_L Shift_R
add    Control = Control_L
! Mod1 is a funny one.  Things like yast look for Alt_L for shortcuts
! but having Meta_L and Alt_L is evil and broken, according to jwz
! (see the xkeycaps man page).
! If we can get away with just Alt_L that seems to be best.
add    Mod1    = Alt_L
add    Mod2    = Num_Lock
add    Mod4    = Super_R
add    Mod5    = Mode_switch ISO_Level3_Shift
