! This ends up being Meta_L for some reason
keycode 0x9c =

! On Fedora 11 this ends up being Super_R for some reason
keycode 0x74 =

clear Shift
clear Lock
clear Control
clear Mod1
clear Mod2
clear Mod3
clear Mod4
clear Mod5

add    Shift   = Shift_L Shift_R
add    Control = Control_L
add    Mod1    = Meta_L Meta_R
!add   Mod2    = Mode_switch
!add    Mod2    = Alt_L Alt_R
add    Mod2    = Alt_L
add    Mod4    = Super_R
